{
  "version": 2,
  "builds": [
    {
      "src": "api/quantum-recon.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/quantum-recon.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/holodeck.html"
    }
  ]
}
